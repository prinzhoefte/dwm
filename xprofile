/usr/bin/pipewire &
/usr/bin/pipewire-pulse &
/usr/bin/wireplumber &
setxkbmap de &

wm=$(printenv | grep "DESKTOP_SESSION")

if [ "$wm" = "DESKTOP_SESSION=xmonad" ]; then
	/home/justin/xmonad/xmonadInit.sh &
	xcompmgr -c &
fi

if [ "$wm" = "DESKTOP_SESSION=dwm" ]; then
	/home/justin/code/dwm/DWM/dwm-bar/dwm_bar.sh &
	xcompmgr -c &
	autorendr -c &
	easyeffects --gapplication-service &
	dunst &
	sleep 2
	feh --bg-scale /home/justin/Bilder/mountain.jpg &
fi
